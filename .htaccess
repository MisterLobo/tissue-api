#AuthUserFile
#AuthName "Restricted Area"
#AuthType Basic
#require valid-user
RewriteEngine on
RewriteCond %{HTTPS} off
RewriteCond %{HTTP_HOST} (www\.)?api-dev.tissueapp.xyz
RewriteRule (.*) https://%{HTTP_HOST}%{REQUEST_URI} [L,R=301]
<IfModule mod_rewrite.c>
    <IfModule mod_negotiation.c>
        Options -MultiViews
    </IfModule>

    RewriteEngine On

    Redirect 301 /app/ /
    Redirect 301 /bootstrap/ /
    Redirect 301 /config/ /
    Redirect 301 /database/ /
    Redirect 301 /public/ /
    Redirect 301 /resources/ /
    Redirect 301 /routes/ /
    Redirect 301 /storage/ /
    Redirect 301 /vendor/ /

    RewriteRule ^ server.php [L]
</IfModule>
